#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<included>
    <root level="INFO">
        <appender-ref ref="CONSOLE_ASYNC"/>
        <appender-ref ref="FILE_ASYNC"/>
        <appender-ref ref="ERROR_FILE_ASYNC"/>
        <if condition='property("SLS_ENDPOINT") != null && !property("SLS_ENDPOINT").isEmpty()'>
            <then>
                <appender-ref ref="ALI_YUN_SLS"/>
            </then>
        </if>
    </root>
</included>
